{
  "name": "pascua",
  "version": "0.0.0-development",
  "description": "Calculadora para los festivos en Colombia",
  "main": "dist/pascua.js",
  "scripts": {
    "docs:serve": "parcel serve ./examples/index.html --out-dir docs",
    "lint": "eslint src",
    "test": "jest --coverage",
    "prebuild:docs": "rimraf docs",
    "prebuild:dist": "rimraf dist",
    "build:dist": "babel src/pascua.js --delete-dir-on-start --out-dir dist",
    "build:docs": "parcel build ./examples/index.html --out-dir docs --no-source-maps",
    "prepublishOnly": "npm-run-all lint test build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/archemiro/pascua/issues"
  },
  "homepage": "https://github.com/archemiro/pascua#readme",
  "author": "Mauricio Robayo <archemiro@gmail.com> (https://www.archemiro.com)",
  "repository": {
    "type": "git",
    "url": "git@github.com:archemiro/pascua.git"
  },
  "keywords": [
    "Colombia",
    "Festivos",
    "Holidays"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-minify": "^0.5.0",
    "codecov": "^3.1.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.17"
  },
  "dependencies": {},
  "files": [
    "dist"
  ]
}
