{
  "name": "colombian-holidays",
  "version": "5.0.5",
  "description": "Colombian holidays",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts --fix",
    "test": "vitest run",
    "build": "tsup src/index.ts --dts --format cjs,esm --out-dir dist",
    "build:cjs": "tsup src/index.ts --format cjs --out-dir dist/cjs",
    "build:esm": "tsup src/index.ts --format esm --out-dir dist/esm",
    "build:types": "tsup src/index.ts --dts-only --out-dir dist/types",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MauricioRobayo/colombian-holidays/issues"
  },
  "homepage": "https://github.com/MauricioRobayo/colombian-holidays",
  "author": "Mauricio Robayo <rfmajo@gmail.com> (https://www.mauriciorobayo.com)",
  "keywords": [
    "Colombian holidays",
    "Festivos en Colombia",
    "Colombia",
    "Festivos",
    "Holidays"
  ],
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "pascua": "^2.1.8"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MauricioRobayo/colombian-holidays.git"
  },
  "release": {
    "branches": "main"
  }
}
